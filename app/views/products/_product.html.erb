<div id="<%= dom_id product %>">
  <p>
    <strong>Product code:</strong>
    <%= product.product_code %>
  </p>

  <p>
    <strong>Product vending code:</strong>
    <%= product.product_vending_code %>
  </p>

  <p>
    <strong>Product name:</strong>
    <%= product.product_name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= product.quantity %>
  </p>

</div>
